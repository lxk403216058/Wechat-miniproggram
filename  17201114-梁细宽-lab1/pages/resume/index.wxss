/* pages/resume/index.wxss */
.header{
  width: 100%;
  height: 400rpx;
  position: relative;
  padding-top: 60rpx;
}
.h-bac{
  position: absolute;
  top:0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%
}
.h-div1{
  width: 50%;
  height: 100rpx;
  float: left;
  margin-left: 50rpx;
}
.h-div2{
  width: 50%;
  height: 260rpx;
  float: left;
  margin-left: 50rpx;
}
.h-left{
  width: 100%;
}
.h-img{
  width: 120rpx;
  height: 120rpx;
  border-radius: 20%;
  margin: 20rpx;
}
.h-right{
  width: 60%;
  color: white;
  padding-top: 40rpx;
}
.h-right view{
  font-size: 32rpx;
  margin: 30rpx auto;
  width: 100%;
  height: 40rpx;
  overflow: hidden;
}
.web{
  position: absolute;
  bottom:  0rpx;
  left: 17%;
}
.main{
  width: 95%;
  height: auto;
  margin-left: 20rpx;
}
.m-view{
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
}
.m-view view{
  float: left;
  width: 66%;
  padding-left: 20rpx;
}
.m-view-text{
  color: #f5f1f1;
}
.m-view-l{
  height: 56rpx;
  width: 56rpx;
  margin: 40rpx 30rpx;
  float: left;
}
.m-view-r{
  height: 60rpx;
  width: 60rpx;
  margin: 30rpx;
  float: right;
}